<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install SYSTEM "http://dev.joomla.org/xml/1.5/module-install.dtd">
<install version="1.5" type="module" client="site">
	<name>J!Analytics</name>
	<creationDate>March 2008</creationDate>
	<author>Dean Tedesco</author>
	<authorEmail>dino@tedesco.net.au</authorEmail>
	<authorUrl>http://www.tedesco.net.au/</authorUrl>
	<copyright>Copyright (C) 2008 Dean Tedesco. All rights reserved.</copyright>
	<license>GNU/GPL</license>
	<version>1.4.0</version>
	<description>JANALYTICS_DESC</description>
	<files>
		<filename module="mod_janalytics">mod_janalytics.php</filename>
		<folder>tmpl</folder>
	</files>
	<languages folder="languages">
		<language tag="en-GB">en-GB.mod_janalytics.ini</language>
		<language tag="de-DE">de-DE.mod_janalytics.ini</language>
		<language tag="es-ES">es-ES.mod_janalytics.ini</language>
		<language tag="fr-FR">fr-FR.mod_janalytics.ini</language>
		<language tag="it-IT">it-IT.mod_janalytics.ini</language>
		<language tag="pt-BR">pt-BR.mod_janalytics.ini</language>
		<language tag="pt-PT">pt-PT.mod_janalytics.ini</language>
	</languages>
	<media></media>
    <params>
        <param name="analyticsDomains" type="radio" default="single" label="TITLEANALYTICSDOMAINS" description="PARAMANALYTICSDOMAINS">
            <option value="single">PARAMANALYTICSSINGLE</option>
            <option value="multiple">PARAMANALYTICSMULTIPLE</option>
        </param>
        <param name="analyticsId" type="text" default="UA-000000-0" label="TITLEANALYTICSID" description="PARAMANALYTICSID" />
        <param name="analyticsSubdomainTracking" type="text" default=".example.com" label="TITLEANALYTICSSUBDOMAINTRACKING" description="PARAMANALYTICSSUBDOMAINTRACKING" />
        <param name="analyticsTrackFiles" type="radio" default="off" label="TITLEANALYTICSTRACKFILES" description="PARAMANALYTICSFILES">
            <option value="on">PARAMANALYTICSON</option>
            <option value="off">PARAMANALYTICSOFF</option>
        </param>
    </params>
</install>
