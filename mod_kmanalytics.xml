<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install SYSTEM "http://dev.joomla.org/xml/1.5/module-install.dtd">
<install version="1.5" type="module" client="site">
	<name>Kingfish Media Analytics</name>
	<creationDate>May 2010</creationDate>
	<author>Kingfish Media Pty Ltd</author>
	<authorEmail>dino@kingfishmedia.com.au</authorEmail>
	<authorUrl>http://www.kingfishmedia.com.au/</authorUrl>
	<copyright>Copyright (C) 2010 Kingfish Media Pty Ltd. All rights reserved.</copyright>
	<license>GNU/GPL</license>
	<version>2.0.0</version>
	<description>KMANALYTICS_DESC</description>
	<files>
		<filename module="mod_kmanalytics">mod_kmanalytics.php</filename>
		<folder>tmpl</folder>
	</files>
	<languages folder="languages">
		<language tag="en-GB">en-GB.mod_kmanalytics.ini</language>
	</languages>
	<media></media>
    <params>
        <param name="gaid" type="text" default="" label="TITLE_ID" description="DESC_ID" />
        <param name="tracking" type="radio" default="single" label="TITLE_TRACKING" description="DESC_TRACKING">
        	<option value="single">OPTION_TRACKING_SINGLE</option>
        	<option value="multisub">OPTION_TRACKING_MULTISUB</option>
        	<option value="multitop">OPTION_TRACKING_MULTITOP</option>
        </param>
        <param name="domain" type="text" default=".kingfishmedia.com.au" label="TITLE_DOMAIN" description="DESC_DOMAIN" />
    </params>
</install>
